{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "HealthChecks": "Error",
      "HealthChecks.*": "Error",
      "System.Net.Http.HttpClient.health-checks.*": "Error"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MoodyDBConnection": "Server=(localdb)\\MSSQLLocalDB;database=Moody;Trusted_Connection=True",
    "WebURL": "http://localhost:3000"
  },
  "TokenExpirationMinutes": 1440,
  "HealthChecksUri": "http://localhost:1234/healthcheck"
}