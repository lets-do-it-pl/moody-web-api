{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MoodyDBConnection": "Server=(localdb)\\MSSQLLocalDB;database=Moody;Trusted_Connection=True"
  },
  "TokenExpirationMinutes": 1440,
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "HealthCheckMoody",
        "Uri": "http://localhost:1234/healthcheck"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }

}
